{
  "name": "invs-modern",
  "version": "1.0.0",
  "description": "Modern INVS Hospital Inventory Management System",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "import:phase1": "tsx scripts/migrate-phase1-data.ts",
    "import:phase2": "tsx scripts/migrate-phase2-data.ts",
    "import:phase3": "tsx scripts/migrate-phase3-data.ts",
    "import:phase4": "tsx scripts/migrate-phase4-drug-master.ts",
    "import:phase5": "tsx scripts/migrate-phase5-lookup-tables.ts",
    "import:phase6": "tsx scripts/migrate-phase6-map-string-to-fk.ts",
    "import:phase7": "tsx scripts/migrate-phase7-tmt-concepts.ts",
    "import:phase8": "tsx scripts/migrate-phase8-map-tmt.ts",
    "import:all": "npm run import:phase1 && npm run import:phase2 && npm run import:phase3 && npm run import:phase4 && npm run import:phase5 && npm run import:phase6 && npm run import:phase7 && npm run import:phase8",
    "import:drugs": "npm run import:phase1 && npm run import:phase2 && npm run import:phase3 && npm run import:phase4",
    "import:lookups": "npm run import:phase5 && npm run import:phase6",
    "import:tmt": "npm run import:phase7 && npm run import:phase8",
    "setup:fresh": "npm run db:migrate && npm run db:seed",
    "setup:full": "npm run db:migrate && npm run db:seed && npm run import:all",
    "tmt:import": "node scripts/import-tmt-data.js",
    "tmt:sync": "node scripts/sync-tmt-updates.js",
    "tmt:import:concepts": "node scripts/import-tmt-data.js --concepts-only",
    "tmt:import:relationships": "node scripts/import-tmt-data.js --relationships-only",
    "tmt:update-codes": "node scripts/import-tmt-data.js --update-codes-only",
    "tmt:stats": "node scripts/import-tmt-data.js --stats-only",
    "tmt:seed-refs": "npx ts-node scripts/seed-tmt-references.ts",
    "his:sync": "npx ts-node scripts/his-integration.ts",
    "migrate:legacy": "node scripts/migrate-legacy-to-modern.js",
    "migrate:legacy:dry-run": "node scripts/migrate-legacy-to-modern.js --dry-run",
    "migrate:legacy:debug": "node scripts/migrate-legacy-to-modern.js --debug",
    "migrate:validate": "node scripts/validate-migration.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "inventory",
    "hospital",
    "postgresql",
    "prisma"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.6.2",
    "mysql2": "^3.15.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "prisma": "^6.16.3",
    "xlsx": "^0.18.5"
  }
}
